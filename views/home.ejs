
<link rel="stylesheet" href="/css/home.css">
<div id="container">
    <section id="links"></section>
    <section id="feed">
        <%- include('_postform'); -%>
        <div id="posts-list-container">
            <ul>
                <% for(post of posts){ %>
                    <%- include('_post'); -%>
                <%}%>
            </ul>
        </div>
        
    </section>
    
    <section id="user-friends">
        <h4>Friends</h4>
        <% for(user of users){%>
            <p>
                <a href="user/profile/<%=user.id%>"><%=user.user_name%></a>
            </p>
        <%}%>
        
    </section>
</div>
